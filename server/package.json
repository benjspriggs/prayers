{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "json-server faker.js --port 5041",
    "build": "tsc",
    "format:general": "node json-to-jsonl.js ../scripts/general_prayers.json",
    "import:general": "node import.js ../scripts/general_prayers.json",
    "import:bahaullah": "node import.js ../scripts/bahaullah_prayers.json",
    "--import:preview": "couchimport --preview true --type json --url http://localhost --database test --transform ./transforms/general_prayer.js --jsonpath \"sections.*\"",
    "import:preview": "cat ./example.jsonl | couchimport --preview true --type jsonl --url http://localhost --delimiter '\\n' --database test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benjspriggs/prayers.git"
  },
