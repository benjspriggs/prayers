  },
  "homepage": "https://github.com/benjspriggs/prayers#readme",
  "devDependencies": {
    "@types/faker": "^4.1.8",
    "@types/pouchdb": "^6.4.0",
    "faker": "^4.1.0",
    "json-server": "^0.15.1"
  },
  "dependencies": {
    "couchimport": "^1.4.1",
    "docopt": "^0.6.2"
  }
}
