{
  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "strictNullChecks": true,
    "allowJs": true,
    "checkJs": true,
    "outDir": "../out/js",
    "rootDir": "./src",
    "module": "es2015",
    "jsx": "react",
    "reactNamespace": "render",
    "baseUrl": "./src",
    "paths": {
      "./*.js": ["./*", "../static/js/*"],
      "rxjs": ["../node_modules/rxjs/index"]
    }
  }
}
